              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Run with needs?"/>
            </properties>
          </component>
        </children>
      </grid>
      <scrollpane id="fc923">
        <constraints border-constraint="Center"/>
        <properties>
          <enabled value="true"/>
          <horizontalScrollBarPolicy value="30"/>
        </properties>
        <border type="empty" title="Jobs">
          <color color="-4473925"/>
        </border>
        <children>
          <component id="ea034" class="javax.swing.JTree" binding="tree">
            <constraints/>
            <properties>
              <enabled value="true"/>
              <foreground color="-12828863"/>
              <rootVisible value="false"/>
              <showsRootHandles value="true"/>
              <toolTipText value="Double-click a job to run it locally."/>
            </properties>
          </component>
        </children>
      </scrollpane>
    </children>
